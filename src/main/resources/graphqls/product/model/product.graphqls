type Product {
    id: UUID!
    name: String!
    categoryId: String!
    description: String!
    images: [String!]
    createdAt: DateTime
    updatedAt: DateTime
}

# Input to create a new Product
input CreateProductInput {
    # Product Name
    name: String!
    # Product parent ID
    categoryId: UUID
    # Product Description
    description: String!
    # Product Images
    images: [String!]
}

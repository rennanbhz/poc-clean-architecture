type Product {
    id: String!
    name: String!
    categoryId: String!
    description: String!
    images: [String!]
    createdAt: String
    updatedAt: String
}

# Input to create a new Product
input CreateProductInput {
    # Product Name
    name: String!
    # Product parent ID
    categoryId: String
    # Product Description
    description: String!
    # Product Images
    images: [String!]
}
